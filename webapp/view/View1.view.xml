<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" controllerName="IntegratejQueryPlugInSapUi5.controller.View1" displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<VBox class="sapUiSmallMargin">
					<Label text="{i18n>phoneNumber}" id="__label0" labelFor="iMobilePhoneNumber"/>
					<Input width="50%" id="iMobilePhoneNumber" value="{ViewModel>/MobilePhoneNumber}"/>
					<Button press="onPress" text="{i18n>press}"></Button>
					<Text text="{ViewModel>/ValidNumber}"></Text>
				</VBox>
			</Page>
		</pages>
	</App>
</mvc:View>